<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">


    <link name="base_footprint"></link>

    <link name="base_link">

        <visual>
            <geometry>
                <mesh filename="package://bumperbot_description/meshes/base_link.STL"></mesh>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"></origin>
        </visual>

        <collision>
            <geometry>
                <mesh filename="package://bumperbot_description/meshes/base_link.STL"></mesh>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"></origin>
        </collision>

        <inertial>
            <origin xyz="0 0 0.04" rpy="0.0 0.25 0.3" />
            <mass value="8.2573504e-01"/>
            <inertia ixx="2.2124416e-02" ixy="-1.2294101e-04" ixz="3.4938785e-04"
                     iyy="2.1193702e-02" iyz="-5.0120904e-05"
                     izz="2.0064271e-02" />
          </inertial>
    </link>

    <joint name="base_joint" type="fixed">
        <parent link="base_footprint" />
        <child link="base_link" />
        <origin xyz="0 0 0.033" rpy="0 0 0"/>
    </joint>

    <link name="left_wheel_link">
        <visual>
            <geometry>
                <mesh filename="package://bumperbot_description/meshes/left_wheel_link.STL"></mesh>
            </geometry>
            <origin xyz="0 0 0" rpy="${-pi/2.0} 0 0"></origin>
        </visual>

        <collision>
            <geometry>
                <sphere radius="0.033"/>
            </geometry>
            <origin xyz="0 0.015 0" rpy="${pi/2.0} 0 0"></origin>
        </collision>

        <inertial>
            <origin xyz="0 0.014 0" rpy="0 0 0" />
            <mass value="0.0530086043217644" />
            <inertia ixx="1.8792911368909E-05"  ixy="3.31256392204584E-10" ixz="-1.14082001662767E-08"
                     iyy="3.11510738759073E-05" iyz="-3.60499245082192E-10"
                     izz="1.88266886146847E-05" />
          </inertial>
    </link>

    <joint name="base_left_wheel_joint" type="continuous">
        <parent link="base_link" />
        <child link="left_wheel_link" />
        <origin xyz="0 0.07 0" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>

    <link name="right_wheel_link">
        <visual>
            <geometry>
                <mesh filename="package://bumperbot_description/meshes/right_wheel_link.STL"></mesh>
            </geometry>
